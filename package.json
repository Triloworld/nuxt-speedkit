{
  "name": "nuxt-speedkit",
  "version": "2.0.8",
  "description": "Nuxt Speedkit takes over the Lighthouse performance optimization of your generated website.",
  "license": "MIT",
  "contributors": [
    {
      "name": "Stephan Gerbeth",
      "email": "stephan.gerbeth@gmail.com"
    },
    {
      "name": "Thorn-Welf Walli",
      "email": "lammpee@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/GrabarzUndPartner/nuxt-speedkit"
  },
  "main": "index.js",
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "dev": "nuxi dev example",
    "build": "nuxi build --config-file example/nuxt.config.js",
    "generate": "nuxi generate --config-file example/nuxt.config.js --target static",
    "lint": "npm run lint:css && npm run lint:es",
    "lint:es": "eslint --ext .js,.vue .",
    "lint:css": "stylelint \"(src|lib)/**/*.vue\"",
    "lint:report": "eslint --ext .js,.vue -f json-relative -o ./eslint-report.json .",
    "test": "npm run lint && jest",
    "jest": "jest",
    "start": "nuxi start --config-file example/nuxt.config.js --target static",
    "start:generate": "npm run generate && npm run start"
  },
  "dependencies": {
    "@nuxt/image": "0.6.0",
    "axios": "0.26.1",
    "bowser": "2.11.0",
    "browserslist-useragent-regexp": "3.0.2",
    "buffer-image-size": "0.6.4",
    "deepmerge": "4.2.2",
    "defu": "6.0.0",
    "dynamic-class-list": "2.0.1",
    "serialize-to-js": "3.1.2",
    "sort-css-media-queries": "2.0.4",
    "vue-lazy-hydration": "2.0.0-beta.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.17.0",
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@fullhuman/postcss-purgecss": "4.1.3",
    "@nuxt/babel-preset-app": "^2.15.8",
    "@nuxt/bridge": "npm:@nuxt/bridge-edge@^3.0.0-27474800.8dd77d7",
    "@nuxt/postcss8": "1.1.3",
    "@nuxt/test-utils": "0.2.2",
    "@nuxtjs/eslint-config": "9.0.0",
    "@nuxtjs/eslint-module": "3.1.0",
    "@nuxtjs/stylelint-module": "4.1.0",
    "babel-jest": "27.5.1",
    "commitlint": "16.2.3",
    "core-js": "3.21.1",
    "eslint": "8.13.0",
    "eslint-formatter-json-relative": "0.1.0",
    "eslint-plugin-no-secrets": "0.8.9",
    "eslint-plugin-no-unsanitized": "4.0.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-sonarjs": "0.13.0",
    "eslint-plugin-xss": "0.1.11",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.7",
    "nuxt-edge": "latest",
    "pinst": "3.0.0",
    "playwright": "1.20.2",
    "postcss-functions": "4.0.2",
    "postcss-html": "1.3.0",
    "postcss-nesting": "10.1.4",
    "postcss-preset-env": "7.4.3",
    "stylelint": "14.6.1",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-standard": "25.0.0",
    "vue-headings": "1.0.0",
    "vue-svg-loader": "0.16.0",
    "webpack-bundle-analyzer": "4.5.0",
    "wicg-inert": "3.1.1"
  },
  "overrides": {
    "@nuxt/bridge": {
      "node-fetch": "2.6.7"
    },
    "ohmyfetch": {
      "node-fetch": "2.6.7"
    }
  }
}
